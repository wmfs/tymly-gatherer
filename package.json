{
  "name": "@wmfs/tymly-gatherer",
  "version": "0.0.0-semantically-released",
  "description": "A package to aid in the generation of Tymly docs",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/tymly-gatherer#readme",
  "keywords": [
    "tymly",
    "docs",
    "package"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-gatherer.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly-gatherer/issues"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "dottie": "2.0.1",
    "glob": "7.1.3",
    "glob-promise": "3.4.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "standard": "12.0.1"
  },
  "scripts": {
    "generate": "set TYMLY_PATH=C:/development/tymly&& node lib",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
