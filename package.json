{
  "name": "@wmfs/tymly-gatherer",
  "version": "0.0.0-semantically-released",
  "description": "A package to collate data about Tymly plugins",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/tymly-gatherer#readme",
  "keywords": [
    "tymly",
    "docs",
    "package"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-gatherer.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly-gatherer/issues"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@wmfs/cardscript-examples": "1.22.0",
    "@wmfs/cardscript-schema": "1.27.0",
    "debug": "4.3.1",
    "jsdoc-api": "7.0.1",
    "dottie": "2.0.2",
    "glob": "7.1.6",
    "glob-promise": "4.1.0",
    "lodash.orderby": "4.6.0",
    "lodash.camelcase": "4.3.0",
    "lodash.set": "4.3.2"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "codecov": "3.8.1",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "8.3.2",
    "nyc": "15.1.0",
    "standard": "16.0.3",
    "semantic-release": "17.4.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@wmfs/tymly": "1.195.0",
    "chai-subset": "1.6.0"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "tymly": {
      "sync": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
